#. Id: 47516-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE=1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47516-Dialogue.mod:0
msgid "Hm hm! Nice sword, /FLATSKIN/..."
msgstr ""

#. Id: 47517-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (47516-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47517-Dialogue.mod:0
msgid "Did you make it yourself?"
msgstr ""

#. Id: 47518-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (47517-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47518-Dialogue.mod:0
msgid "How precious... The /FLATSKIN/ thinks /HE/ can fight..."
msgstr ""

#. Id: 47519-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (47518-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IN_COMBAT=0, Tag: -1
#: 47519-Dialogue.mod:0
msgid "Hm! That's it, run, /FLATSKIN/. It's what your kind does best..."
msgstr ""

#. Id: 47520-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (47518-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47520-Dialogue.mod:0
msgid "You don't run from me? Then use that sword and fight me! "
msgstr ""

#. Id: 47521-Dialogue.mod
#. -- Index: 4.0
#. -- Parent line: 3 (47520-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: IS_RUNNING=0, Tag: -1
#: 47521-Dialogue.mod:0
msgid "Fight for your honour, the honour your ancestors lost for you long ago..."
msgstr ""

#. Id: 47522-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47522-Dialogue.mod:0
msgid "Look at the outsiders... Thinking they can wield a sword..."
msgstr ""

#. Id: 47522-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47522-Dialogue.mod:1
msgid "Amusing. The outsiders think they can wield a sword..."
msgstr ""

#. Id: 47522-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47522-Dialogue.mod:2
msgid "A /FLATSKIN/ cannot wield a sword..."
msgstr ""

#. Id: 47522-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47522-Dialogue.mod:3
msgid "Outsiders should not play with swords..."
msgstr ""

#. Id: 47522-Dialogue.mod
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47522-Dialogue.mod:5
msgid "You should not play with swords, /FLATSKIN/..."
msgstr ""

#. Id: 47522-Dialogue.mod
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47522-Dialogue.mod:6
msgid "That sword looks a little big for you, /FLATSKIN/..."
msgstr ""

#. Id: 47524-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (47522-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47524-Dialogue.mod:0
msgid "Only a real warrior deserves to wield a sword!"
msgstr ""

#. Id: 47524-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (47522-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47524-Dialogue.mod:1
msgid "Nobody but a real warrior should hold a sword!"
msgstr ""

#. Id: 47524-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (47522-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47524-Dialogue.mod:2
msgid "To hold a sword is an honour reserved only for true warriors!"
msgstr ""

#. Id: 47524-Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (47522-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47524-Dialogue.mod:3
msgid "You wield a sword, you should be able to fight with it!"
msgstr ""

#. Id: 47525-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (47524-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47525-Dialogue.mod:0
msgid "So show me how you fight, /FLATSKIN/!"
msgstr ""

#. Id: 47525-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (47524-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47525-Dialogue.mod:1
msgid "So do battle with me, /FLATSKIN/! Prove to me that you deserve to hold a sword!"
msgstr ""

#. Id: 47525-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (47524-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47525-Dialogue.mod:2
msgid "Let me show you how a REAL warrior uses one!"
msgstr ""

#. Id: 47526-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (47522-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47526-Dialogue.mod:0
msgid "How about we teach /HIM/ how to use it!"
msgstr ""

#. Id: 47526-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (47522-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47526-Dialogue.mod:1
msgid "Let's teach them how a REAL warrior uses one!"
msgstr ""

#. Id: 47526-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (47522-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47526-Dialogue.mod:2
msgid "Let's see how they handle one in a real fight!"
msgstr ""

#. Id: 47527-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47527-Dialogue.mod:0
msgid "The /FLATSKIN/ comes armed... Time to test my new sword!"
msgstr ""

#. Id: 47527-Dialogue.mod
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47527-Dialogue.mod:1
msgid "These ones carry weapons... Looks like the outsiders want to play swordsman!"
msgstr ""

#. Id: 47527-Dialogue.mod
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47527-Dialogue.mod:2
msgid "Let us teach you how to use those swords, outsiders!"
msgstr ""

#. Id: 47527-Dialogue.mod
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47527-Dialogue.mod:3
msgid "Think yourselves, swordsmen, outsiders? Then try me! Raise your weapons!"
msgstr ""

#. Id: 47527-Dialogue.mod
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47527-Dialogue.mod:4
msgid "You tread with a sword, you should expect to use it, outsiders!"
msgstr ""

#. Id: 47530-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (47527-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47530-Dialogue.mod:0
msgid "Doom to the spineless! For Kral!"
msgstr ""

#. Id: 47530-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (47527-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47530-Dialogue.mod:1
msgid "Honour to Kral!"
msgstr ""

#. Id: 47530-Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (47527-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. Id: 56426-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (56425-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek revenge mission, esata killed/ caged
#: 47530-Dialogue.mod:2 56426-Dialogue.mod:2
msgid "Blood for Kral!"
msgstr ""

#. Id: 47528-Dialogue.mod
#. -- Index: 0.0
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE>1, Tag: -1
#. -- Condition: Target Race(s): Greenlander or Hive Prince
#: 47528-Dialogue.mod:0
msgid "/BATTLECRY/! CHARGE!"
msgstr ""

#. Id: 47529-Dialogue.mod
#. -- Index: 1.0
#. -- Parent line: 0 (47528-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Shek picking fight
#: 47529-Dialogue.mod:0
msgid "Flee, outsiders! It's what your kind does best afterall!"
msgstr ""

