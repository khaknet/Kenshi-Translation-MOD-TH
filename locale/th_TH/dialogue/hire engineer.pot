#. Id: 16867-Recruits Dialogue.mod
#. -- Index: 1.2
#. -- Parent line: 0 (1765-gamedata.base)
#. -- Speaker: Target (1)
#. -- Group title: hire engineer
#: 16867-Recruits?Dialogue.mod:2
msgid "Can you fight as well?"
msgstr ""

#. Id: 16867-Recruits Dialogue.mod
#. -- Index: 1.3
#. -- Parent line: 0 (1765-gamedata.base)
#. -- Speaker: Target (1)
#. -- Group title: hire engineer
#: 16867-Recruits?Dialogue.mod:3
msgid "What about combat? Can you fight?"
msgstr ""

#. Id: 16867-Recruits Dialogue.mod
#. -- Index: 1.4
#. -- Parent line: 0 (1765-gamedata.base)
#. -- Speaker: Target (1)
#. -- Group title: hire engineer
#: 16867-Recruits?Dialogue.mod:4
msgid "Depends. Can you fight?"
msgstr ""

#. Id: 16867-Recruits Dialogue.mod
#. -- Index: 1.5
#. -- Parent line: 0 (1765-gamedata.base)
#. -- Speaker: Target (1)
#. -- Group title: hire engineer
#: 16867-Recruits?Dialogue.mod:5
msgid "First things first, can you fight?"
