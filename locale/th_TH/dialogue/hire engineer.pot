msgid "I'm an experienced engineer, looking for work. Got any?"
msgstr ""

#. Id: 1765-gamedata.base
#. -- Index: 0.1
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1765-gamedata.base:1
msgid "Looking to hire an engineer? I'm looking for work if you've got any..."
msgstr ""

#. Id: 1765-gamedata.base
#. -- Index: 0.2
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1765-gamedata.base:2
msgid "/HI/ /OUTLANDER/, I'm a pretty good engineer if you need one. What are you saying?"
msgstr ""

#. Id: 1765-gamedata.base
#. -- Index: 0.3
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1765-gamedata.base:3
msgid "Engineer looking for work! Need an engineer? I can engineer stuff for you...."
msgstr ""

#. Id: 1765-gamedata.base
#. -- Index: 0.4
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1765-gamedata.base:4
msgid "I can handle all your engineering needs. Interested?"
msgstr ""

#. Id: 1765-gamedata.base
#. -- Index: 0.5
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1765-gamedata.base:5
msgid "Interested in hiring an engineer? I'm looking for work if you've got any..."
msgstr ""

#. Id: 1765-gamedata.base
#. -- Index: 0.6
#. -- Parent line: -
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1765-gamedata.base:6
msgid "Need some walls built? I can handle 'em for you..."
msgstr ""

#. Id: 1766-gamedata.base
#. -- Index: 1.2
#. -- Parent line: 0 (1765-gamedata.base)
#. -- Speaker: Target (1)
#. -- Group title: hire engineer
#: 1766-gamedata.base:2
msgid "Depends, how much?"
msgstr ""

#. Id: 1768-gamedata.base
#. -- Index: 2.0
#. -- Parent line: 1 (1766-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1768-gamedata.base:0
msgid "6000. I'm not shy of a bit of manual labour either."
msgstr ""

#. Id: 1768-gamedata.base
#. -- Index: 2.1
#. -- Parent line: 1 (1766-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1768-gamedata.base:1
msgid "I charge 6000 Cats. How about it?"
msgstr ""

#. Id: 1768-gamedata.base
#. -- Index: 2.2
#. -- Parent line: 1 (1766-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1768-gamedata.base:2
msgid "6000 Cats. Whaddya thinking?"
msgstr ""

#. Id: 1768-gamedata.base
#. -- Index: 2.3
#. -- Parent line: 1 (1766-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1768-gamedata.base:3
msgid "6000 Cats. Always open to a bit of manual labour if it needs doing too."
msgstr ""

#. Id: 1768-gamedata.base
#. -- Index: 2.4
#. -- Parent line: 1 (1766-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1768-gamedata.base:4
msgid "Just a 6000 advance. Keep me fed with a roof over my head, and I don't need nothing else."
msgstr ""

#. Id: 1768-gamedata.base
#. -- Index: 2.5
#. -- Parent line: 1 (1766-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1768-gamedata.base:5
msgid "6000 Cats. How about it, am I hired?"
msgstr ""

#. Id: 1768-gamedata.base
#. -- Index: 2.6
#. -- Parent line: 1 (1766-gamedata.base)
#. -- Speaker: Me (0)
#. -- Group title: hire engineer
#: 1768-gamedata.base:6
msgid "6000 Cats and you got yourself a fine labourer, best in the city!"
msgstr ""

#. Id: 1769-gamedata.base
#. -- Index: 3.0
#. -- Parent line: 2 (1768-gamedata.base)
#. -- Speaker: Target (1)
#. -- Group title: hire engineer
#. -- Condition: Who: TARGET, Condition: PLAYERMONEY>6000, Tag: -1
#: 1769-gamedata.base:0
msgid "Sure, come with me."
msgstr ""

