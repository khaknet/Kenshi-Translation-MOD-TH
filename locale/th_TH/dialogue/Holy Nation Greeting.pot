#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE=1, Tag: -1
#: 17092-Dialogue.mod:0
msgid "You WILL be discovered if you are a heretic, /OUTLANDER/!"
msgstr ""

#. Id: 17092-Dialogue.mod
#. -- Index: 1.1
#. -- Parent line: 0 (17091-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Who: TARGET, Condition: TARGET_IN_TALKING_RANGE=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: IS_SAME_RACE_AS_ME=1, Tag: -1
#. -- Condition: Who: TARGET, Condition: SQUAD_SIZE=1, Tag: -1
#: 17092-Dialogue.mod:1
msgid "Your heretic ways WILL be unraveled if your faith is not pure, /OUTLANDER/!"
msgstr ""

#. Id: 17090-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (17092-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Who: TARGET, Condition: TARGET_IN_TALKING_RANGE=1, Tag: -1
#: 17090-Dialogue.mod:0
msgid "But I'm sure you're no heretic... blessings upon you, /BROTHER/"
msgstr ""

#. Id: 17090-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (17092-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Who: TARGET, Condition: TARGET_IN_TALKING_RANGE=1, Tag: -1
#: 17090-Dialogue.mod:1
msgid "Stay holy... love and devotion, /BROTHER/"
msgstr ""

#. Id: 17090-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (17092-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Who: TARGET, Condition: TARGET_IN_TALKING_RANGE=1, Tag: -1
#: 17090-Dialogue.mod:2
msgid "With that said... divine blessings, /BROTHER/"
msgstr ""

#. Id: 42565-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42562-Dialogue.mod)
