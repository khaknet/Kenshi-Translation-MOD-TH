#. -- Group title: Holy Nation Greeting
#. -- Condition: Who: TARGET, Condition: MET_TARGET_BEFORE=0, Tag: -1
#. -- Condition: Target Race(s): Greenlander
#: 42565-Dialogue.mod:0
msgid "Stop. I need to speak with you."
msgstr ""

#. Id: 43728-Dialogue.mod
#. -- Index: 2.0
#. -- Parent line: 1 (42562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 43728-Dialogue.mod:0
msgid "I'll warn you once, and only once, /BROTHER/..."
msgstr ""

#. Id: 43728-Dialogue.mod
#. -- Index: 2.1
#. -- Parent line: 1 (42562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 43728-Dialogue.mod:1
msgid "I'll be watching you, /BROTHER/. Heed my words..."
msgstr ""

#. Id: 43728-Dialogue.mod
#. -- Index: 2.2
#. -- Parent line: 1 (42562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 43728-Dialogue.mod:2
msgid "You're travelling with... 'them'?"
msgstr ""

#. Id: 43728-Dialogue.mod
#. -- Index: 2.3
#. -- Parent line: 1 (42562-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 43728-Dialogue.mod:3
msgid "Is this... 'being'.. with you?"
msgstr ""

#. Id: 42869-Dialogue.mod
#. -- Index: 3.0
#. -- Parent line: 2 (43728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 42869-Dialogue.mod:0
msgid "Watch your beasts. The dark ones aren't so welcome in this city..."
msgstr ""

#. Id: 42869-Dialogue.mod
#. -- Index: 3.1
#. -- Parent line: 2 (43728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 42869-Dialogue.mod:1
msgid "Clean up your beasts, I'll have no trouble within these walls..."
msgstr ""

#. Id: 42869-Dialogue.mod
#. -- Index: 3.2
#. -- Parent line: 2 (43728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 42869-Dialogue.mod:2
msgid "Control your servants... The foreign beasts aren't so welcome in this town..."
msgstr ""

#. Id: 42869-Dialogue.mod
#. -- Index: 3.3
#. -- Parent line: 2 (43728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 42869-Dialogue.mod:3
msgid "Control your beasts... I want no trouble from them within our walls..."
msgstr ""

#. Id: 42869-Dialogue.mod
#. -- Index: 3.4
#. -- Parent line: 2 (43728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 42869-Dialogue.mod:4
msgid "You're playing a dangerous game bringing THEM here..."
msgstr ""

#. Id: 42869-Dialogue.mod
#. -- Index: 3.5
#. -- Parent line: 2 (43728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 42869-Dialogue.mod:5
msgid "The people of this city have little patience for the dark ones... Keep them under close watch..."
msgstr ""

#. Id: 42869-Dialogue.mod
#. -- Index: 3.6
#. -- Parent line: 2 (43728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 42869-Dialogue.mod:6
msgid "Dark ones aren't welcome here... And by that I mean your choice of company."
msgstr ""

#. Id: 42869-Dialogue.mod
#. -- Index: 3.7
#. -- Parent line: 2 (43728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 42869-Dialogue.mod:7
msgid "Bringing dark ones into the city will land you in a lot of trouble..."
msgstr ""

#. Id: 42869-Dialogue.mod
#. -- Index: 3.8
#. -- Parent line: 2 (43728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 42869-Dialogue.mod:8
msgid "Keep your beasts in order. Their kind isn't so welcome around here."
msgstr ""

#. Id: 42869-Dialogue.mod
#. -- Index: 3.9
#. -- Parent line: 2 (43728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 42869-Dialogue.mod:9
msgid "Clean up your beasts. I want no trouble here..."
msgstr ""

#. Id: 42869-Dialogue.mod
#. -- Index: 3.10
#. -- Parent line: 2 (43728-Dialogue.mod)
#. -- Speaker: Me (0)
#. -- Group title: Holy Nation Greeting
#. -- Condition: Target Race(s): Greenlander
#: 42869-Dialogue.mod:10
msgid "The people of this city don't take kindly to beast-lovers. I suggest you tread carefully."
msgstr ""

